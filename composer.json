{
    "name": "zinntikumugai/insight-api-php",
    "description": "Using insight api library",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "insight",
        "crypto",
        "http",
        "curl"
    ],
    "authors": [
        {
            "name": "zinntikumugai",
            "email": "uesitananame55@gmail.com"
        }
    ],

    "require": {
        "php": ">=7.0.0",
        "mpyw/co": "^1.5"
    },
    "require-dev": {
        "php": ">=7.0.0",
        "codeception/codeception": "^2.3",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "zinntikumugai\\InsightApi\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/codecept run unit --coverage --coverage-xml"
        ]
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    }
}
